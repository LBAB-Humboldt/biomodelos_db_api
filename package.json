{
  "name": "BioModelos_mongoDB_API",
  "version": "2.0.0",
  "description": "RESTful server API for Biomodels.",
  "main": "index.js",
  "author": "Valentina Grajales Olarte - https://github.com/valegrajales",
  "license": "MIT",
  "scripts": {
    "start": "nodemon src/app.js --exec babel-node -e js,yaml",
    "test": "mocha -R spec src/**/*.spec.js",
    "coverage": "istanbul cover _mocha $(find ./src -name \"*.spec.js\" -not -path \"./node_modules/*\") ",
    "metric": "nodejs-dashboard -- node -r nodejs-dashboard src/app.js",
    "swaggerEdit": "swagger_swagger_fileName=src/swagger/swagger.yaml swagger project edit",
    "swaggerRun": "swagger_swagger_fileName=src/swagger/swagger.yaml swagger project start",
    "swaggerRunMockup": "swagger_swagger_fileName=src/swagger/swagger.yaml swagger project start -m"
  },
  "dependencies": {
    "babel-register": "^6.24.1",
    "body-parser": "1.17.2",
    "compression": "^1.6.2",
    "convict": "^4.0.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "debug": "^2.6.8",
    "express": "4.15.3",
    "geojson": "*",
    "helmet": "^3.6.1",
    "method-override": "^2.3.9",
    "mongodb": "^2.2.29",
    "mongoose": "4.11.1",
    "morgan": "^1.8.2",
    "swagger-express-mw": "^0.7.0",
    "swagger-jsdoc": "^1.9.5",
    "swagger-tools": "^0.10.1",
    "swagger-ui-express": "^2.0.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "assert": "*",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "connect-livereload": "^0.6.0",
    "eslint": "^4.1.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "flow": "^0.2.3",
    "jshint": "^2.9.5",
    "mocha": "^3.4.2",
    "nodejs-dashboard": "^0.4.1",
    "nodemon": "^1.11.0",
    "prettier": "^1.5.2",
    "should": "^11.2.1",
    "supertest": "^3.0.0",
    "swagger": "^0.7.5",
    "swagger-editor": "^3.0.13"
  }
