{
  "name": "BioModelos_mongoDB_API",
  "version": "2.0.4",
  "description": "RESTful server API for Biomodels.",
  "main": "index.js",
  "author": "Valentina Grajales Olarte - https://github.com/valegrajales",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/LBAB-Humboldt/biomodelos_db_api.git"
  },
  "scripts": {
    "start": "node dist/server/app.js",
    "build": "gulp build",
    "dev": "nodemon src/app.js --exec babel-node -e js,yaml",
    "test": "mocha -R spec src/**/*.spec.js",
    "coverage": "istanbul cover _mocha $(find ./src -name \"*.spec.js\" -not -path \"./node_modules/*\") ",
    "metric": "nodejs-dashboard -- node -r nodejs-dashboard src/app.js",
    "swaggerEdit": "swagger_swagger_fileName=src/swagger/swagger.yaml swagger project edit",
    "swaggerRun": "swagger_swagger_fileName=src/swagger/swagger.yaml swagger project start",
    "swaggerRunMockup": "swagger_swagger_fileName=src/swagger/swagger.yaml swagger project start -m"
  },
  "dependencies": {
    "body-parser": "1.18.2",
    "compression": "^1.7.1",
    "convict": "^4.0.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "express": "4.16.2",
    "geojson": "*",
    "helmet": "^3.9.0",
    "method-override": "^2.3.10",
    "mongodb": "^3.0.0-rc0",
    "mongoose": "4.13.7",
    "morgan": "^1.9.0",
    "serve-favicon": "^2.4.5",
    "swagger-express-mw": "^0.7.0",
    "swagger-jsdoc": "^1.9.7",
    "swagger-tools": "^0.10.3",
    "swagger-ui-express": "^2.0.13",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "assert": "*",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "connect-livereload": "^0.6.0",
    "eslint": "^4.13.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "flow": "^0.2.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-clean": "^0.3.2",
    "gulp-cli": "^1.4.0",
    "gulp-json-modify": "^1.0.2",
    "gulp-minifier": "^0.2.2",
    "jshint": "^2.9.5",
    "mocha": "^4.0.1",
    "nodejs-dashboard": "^0.4.3",
    "nodemon": "^1.13.3",
    "prettier": "^1.9.2",
    "should": "^13.1.3",
    "supertest": "^3.0.0",
    "swagger": "^0.7.5",
    "swagger-editor": "^3.1.20"
  }
}
